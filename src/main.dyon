


fn main() {
    println("Hello world!")

    list := sift i 10 { i+0 }
    println(list)

    list := sift i [10, 20) { i+0 }
    println(list)

    say_hi()

    println(f(2))
    for i 20 { println(fib(i)) }
}

f(x) = x^2

fib(x) = {
    if x <= 0 { 0 }
    else if x == 1 { 1 }
    else { fib(x-1) + fib(x-2) }
}


fn say_hi() {
    println("hi!")
}
